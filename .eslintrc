{
	"extends": ["next", "next/core-web-vitals", "eslint:recommended", "prettier"],
	"globals": {
		"React": "readonly"
	},
	"plugins": ["react"],
	"rules": {
		"no-unused-vars": [
			1,
			{
				"args": "after-used",
				"argsIgnorePattern": "^_"
			}
		],
		"max-len": [
			1,
			{
				"code": 120
			}
		],
		"no-shadow": 0,
		"no-console": 1,
		"semi": 1,
		"object-curly-spacing": [1, "always"],
		"computed-property-spacing": [1, "always"]
	}
}
